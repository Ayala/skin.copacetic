<?xml version="1.0" encoding="UTF-8"?>
<includes>

	<default type="label">
		<height>36</height>
		<align>left</align>
		<aligny>center</aligny>
		<font>Font36</font>
		<textcolor>$VAR[FocusedColor]</textcolor>
		<disabledcolor>$VAR[DisabledColor]</disabledcolor>
		<selectedcolor>$VAR[SelectedColor]</selectedcolor>
		<scroll>false</scroll>
	</default>

	<default type="fadelabel">
		<height>36</height>
		<align>left</align>
		<aligny>center</aligny>
		<font>Font36</font>
		<textcolor>$VAR[FocusedColor]</textcolor>
		<scrollout>false</scrollout>
		<scrollspeed>40</scrollspeed>
		<pauseatend>2000</pauseatend>
	</default>

	<default type="button">
		<width>400</width>
		<height>75</height>
		<align>left</align>
		<aligny>center</aligny>
		<font>Font36</font>
		<label>-</label>
		<textoffsetx>10</textoffsetx>
		<textcolor>$VAR[FocusedColor]</textcolor>
		<focusedcolor>$VAR[FocusedColor]</focusedcolor>
		<invalidcolor>$VAR[DisabledColor]</invalidcolor>
		<disabledcolor>$VAR[DisabledColor]</disabledcolor>
		<texturefocus colordiffuse="$VAR[ButtonColor]">views/white_100.png</texturefocus>
		<texturenofocus>-</texturenofocus>
		<pulseonselect>false</pulseonselect>
	</default>

	<default type="togglebutton">
		<width>400</width>
		<height>75</height>
		<align>left</align>
		<aligny>center</aligny>
		<label>-</label>
		<textoffsetx>10</textoffsetx>
		<font>Font36</font>
		<textcolor>$VAR[FocusedColor]</textcolor>
		<focusedcolor>$VAR[FocusedColor]</focusedcolor>
		<disabledcolor>$VAR[DisabledColor]</disabledcolor>
		<texturefocus colordiffuse="$VAR[ButtonColor]">views/white_100.png</texturefocus>
		<texturenofocus>-</texturenofocus>
		<alttexturefocus colordiffuse="$VAR[ButtonColor]">views/white_100.png</alttexturefocus>
		<alttexturenofocus>-</alttexturenofocus>
		<pulseonselect>false</pulseonselect>
	</default>

	<default type="radiobutton">
		<width>400</width>
		<height>75</height>
		<align>left</align>
		<aligny>center</aligny>
		<font>Font36</font>
		<label>-</label>
		<textoffsetx>10</textoffsetx>
		<textcolor>$VAR[FocusedColor]</textcolor>
		<focusedcolor>$VAR[FocusedColor]</focusedcolor>
		<disabledcolor>$VAR[DisabledColor]</disabledcolor>
		<radiowidth />
		<radioheight />
		<texturefocus colordiffuse="$VAR[ButtonColor]">views/white_100.png</texturefocus>
		<texturenofocus>-</texturenofocus>
		<textureradioonfocus/>
		<textureradioofffocus/>
		<textureradioonnofocus/>
		<textureradiooffnofocus/>
		<textureradioondisabled/>
		<textureradiooffdisabled/>
		<pulseonselect>false</pulseonselect>
	</default>

	<default type="spincontrolex">
		<width>400</width>
		<height>75</height>
		<align>left</align>
		<aligny>center</aligny>
		<label>-</label>
		<font>Font36</font>
		<textoffsetx>10</textoffsetx>
		<textcolor>$VAR[FocusedColor]</textcolor>
		<focusedcolor>$VAR[FocusedColor]</focusedcolor>
		<disabledcolor>$VAR[DisabledColor]</disabledcolor>
		<spinwidth>55</spinwidth>
		<spinheight>75</spinheight>
		<texturefocus colordiffuse="$VAR[ButtonColor]">views/white_100.png</texturefocus>
		<texturenofocus>-</texturenofocus>
		<textureup colordiffuse="$VAR[FocusedColor]">dialogs/spin_up.png</textureup>
		<texturedown colordiffuse="$VAR[FocusedColor]">dialogs/spin_down.png</texturedown>
		<textureupfocus colordiffuse="$VAR[FocusedColor]">dialogs/spin_up.png</textureupfocus>
		<texturedownfocus colordiffuse="$VAR[FocusedColor]">dialogs/spin_down.png</texturedownfocus>
		<textureupdisabled colordiffuse="$VAR[DisabledColor]">dialogs/spin_up.png</textureupdisabled>
		<texturedowndisabled colordiffuse="$VAR[DisabledColor]">dialogs/spin_down.png</texturedowndisabled>
		<reverse>yes</reverse>
		<pulseonselect>false</pulseonselect>
	</default>

	<default type="spincontrol">
		<height>75</height>
		<width>75</width>
		<align>left</align>
		<aligny>center</aligny>
		<spinwidth>43</spinwidth>
		<spinheight>60</spinheight>
		<font>Font36</font>
		<textoffsetx>10</textoffsetx>
		<textcolor>$VAR[FocusedColor]</textcolor>
		<focusedcolor>$VAR[FocusedColor]</focusedcolor>
		<disabledcolor>$VAR[DisabledColor]</disabledcolor>
		<spinwidth>43</spinwidth>
		<spinheight>60</spinheight>
		<textureup colordiffuse="$VAR[FocusedColor]">dialogs/spin_up.png</textureup>
		<texturedown colordiffuse="$VAR[FocusedColor]">dialogs/spin_down.png</texturedown>
		<textureupfocus colordiffuse="$VAR[FocusedColor]">dialogs/spin_up.png</textureupfocus>
		<texturedownfocus colordiffuse="$VAR[FocusedColor]">dialogs/spin_down.png</texturedownfocus>
		<reverse>yes</reverse>
		<pulseonselect>false</pulseonselect>
	</default>

	<default type="sliderex">
		<height>75</height>
		<width>183</width>
		<align>left</align>
		<aligny>center</aligny>
		<sliderwidth>230</sliderwidth>
		<sliderheight>23</sliderheight>
		<font>Font36</font>
		<textcolor>$VAR[FocusedColor]</textcolor>
		<focusedcolor>$VAR[FocusedColor]</focusedcolor>
		<disabledcolor>$VAR[DisabledColor]</disabledcolor>
		<texturefocus colordiffuse="$VAR[ButtonColor]">views/white_100.png</texturefocus>
		<texturenofocus>-</texturenofocus>
		<textoffsetx>10</textoffsetx>
	</default>

	<default type="slider">
		<width>360</width>
		<height>23</height>
		<texturefocus colordiffuse="$VAR[ButtonColor]">views/white_100.png</texturefocus>
		<texturenofocus>-</texturenofocus>
	</default>

	<default type="progress">
		<height>44</height>
		<texturebg>-</texturebg>
		<lefttexture>-</lefttexture>
		<righttexture>-</righttexture>
		<overlaytexture>-</overlaytexture>
	</default>

	<default type="textbox">
		<font>Font27</font>
		<textcolor>$VAR[FocusedColor]</textcolor>
		<autoscroll delay="8000" time="4000" repeat="12000">Skin.HasSetting(ScrollPlot)</autoscroll>
	</default>

	<default type="edit">
		<height>75</height>
		<align>left</align>
		<aligny>center</aligny>
		<font>Font36</font>
		<hinttext/>
		<textoffsetx>10</textoffsetx>
		<textcolor>$VAR[FocusedColor]</textcolor>
		<focusedcolor>$VAR[ButtonColor]</focusedcolor>
		<disabledcolor>$VAR[DisabledColor]</disabledcolor>
		<invalidcolor>$VAR[DisabledColor]</invalidcolor>
		<texturefocus colordiffuse="$VAR[FocusedColor]">views/white_100.png</texturefocus>
		<texturenofocus>-</texturenofocus>
		<pulseonselect>no</pulseonselect>
	</default>

</includes>