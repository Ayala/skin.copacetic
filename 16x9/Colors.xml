	</variable>
	<variable name="FocusedColor">
		<value condition="String.IsEmpty(Skin.String(FocusedColor))">ff46ef75</value>
		<value>$INFO[Skin.String(FocusedColor)]</value>
	</variable>
	<variable name="DarkColor">
		<value condition="String.IsEmpty(Skin.String(FocusedColor))">ff1b102b</value>
