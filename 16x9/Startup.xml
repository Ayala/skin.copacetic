<?xml version="1.0" encoding="UTF-8"?>
<window>
	<!-- startup -->
	<onload>Skin.Reset(HideSettings)</onload>
	<onload>ReplaceWindow($INFO[System.StartupWindow])</onload>
	<!--Default Skin Settings-->
	<onload condition="String.IsEmpty(Skin.String(Theme))">Skin.SetString(Theme,Dark)</onload>
	<onload condition="String.IsEmpty(Skin.String(ListView_Display))">Skin.SetString(ListView_Display,Background)</onload>
	<onload condition="String.IsEmpty(Skin.String(ListView_Display_Music))">Skin.SetString(ListView_Display_Music,Background)</onload>
	<onload condition="String.IsEmpty(Skin.String(ListView_Details))">Skin.SetString(ListView_Details,TitleAndInfo)</onload>
	<onload condition="String.IsEmpty(Skin.String(ListView_Flags))">Skin.SetString(ListView_Flags,All)</onload>
	<onload condition="String.IsEmpty(Skin.String(BillboardView_Details))">Skin.SetString(BillboardView_Details,All)</onload>
	<onload condition="String.IsEmpty(Skin.String(BillboardView_Logo))">Skin.SetString(BillboardView_Logo,Off)</onload>
	<onload condition="String.IsEmpty(Skin.String(BillboardView_Flags))">Skin.SetString(BillboardView_Flags,All)</onload>
	<onload condition="String.IsEmpty(Skin.String(ShowcaseView_Display))">Skin.SetString(ShowcaseView_Display,Square)</onload>
	<onload condition="String.IsEmpty(Skin.String(ShowcaseView_Details))">Skin.SetString(ShowcaseView_Details,All)</onload>
	<onload condition="String.IsEmpty(Skin.String(ShowcaseView_Logo))">Skin.SetString(ShowcaseView_Logo,Off)</onload>
	<onload condition="String.IsEmpty(Skin.String(ShowcaseView_Flags))">Skin.SetString(ShowcaseView_Flags,All)</onload>
	<onload condition="String.IsEmpty(Skin.String(StripView_Display))">Skin.SetString(StripView_Display,Posters)</onload>
	<onload condition="String.IsEmpty(Skin.String(StripView_Display_Music))">Skin.SetString(StripView_Display_Music,Fanart)</onload>
	<onload condition="String.IsEmpty(Skin.String(StripView_Details))">Skin.SetString(StripView_Details,All)</onload>
	<onload condition="String.IsEmpty(Skin.String(StripView_Logo))">Skin.SetString(StripView_Logo,Off)</onload>
	<onload condition="String.IsEmpty(Skin.String(StripView_Flags))">Skin.SetString(StripView_Flags,All)</onload>
	<onload condition="String.IsEmpty(Skin.String(GridView_Display))">Skin.SetString(GridView_Display,Fanart)</onload>
	<onload condition="String.IsEmpty(Skin.String(GridView_Display_Music))">Skin.SetString(GridView_Display_Music,Fanart)</onload>
	<onload condition="String.IsEmpty(Skin.String(GridView_Details))">Skin.SetString(GridView_Details,Off)</onload>
	<onload>SetProperty(SkinInitStarted,1,Home)</onload>
</window>
