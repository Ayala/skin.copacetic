<?xml version="1.0" encoding="UTF-8"?>
<window>
	<!-- smartplaylisteditor -->
	<defaultcontrol always="true">22</defaultcontrol>
	<controls>
		<!-- Edit Rule Button -->
		<control type="button" id="15">
			<left>-10</left>
			<top>-10</top>
			<width>1</width>
			<height>1</height>
		</control>
		<!-- Add Rule Button -->
		<control type="button" id="13">
			<left>-10</left>
			<top>-10</top>
			<width>1</width>
			<height>1</height>
			<onup condition="!IsEmpty(Container(10).ListItemNoWrap(-1).Label)">Control.Move(10,-1)</onup>
			<onup condition="!IsEmpty(Container(10).ListItemNoWrap(1).Label)">SetFocus(10)</onup>
			<onup condition="!IsEmpty(Container(10).ListItemNoWrap(1).Label)">SetFocus(13)</onup>
			<ondown condition="!IsEmpty(Container(10).ListItemNoWrap(1).Label)">Control.Move(10,1)</ondown>
			<ondown condition="!IsEmpty(Container(10).ListItemNoWrap(1).Label)">SetFocus(10)</ondown>
			<ondown condition="!IsEmpty(Container(10).ListItemNoWrap(1).Label)">SetFocus(13)</ondown>
			<ondown condition="IsEmpty(Container(10).ListItemNoWrap(1).Label)">9001</ondown>
			<onleft>10</onleft>
			<onright>14</onright>
		</control>
		<!-- Remove Rule Button -->
		<control type="button" id="14">
			<left>-10</left>
			<top>-10</top>
			<width>1</width>
			<height>1</height>
			<onup condition="!IsEmpty(Container(10).ListItemNoWrap(-1).Label)">Control.Move(10,-1)</onup>
			<onup condition="!IsEmpty(Container(10).ListItemNoWrap(1).Label)">SetFocus(10)</onup>
			<onup condition="!IsEmpty(Container(10).ListItemNoWrap(1).Label)">SetFocus(14)</onup>
			<ondown condition="!IsEmpty(Container(10).ListItemNoWrap(1).Label)">Control.Move(10,1)</ondown>
			<ondown condition="!IsEmpty(Container(10).ListItemNoWrap(1).Label)">SetFocus(10)</ondown>
			<ondown condition="!IsEmpty(Container(10).ListItemNoWrap(1).Label)">SetFocus(14)</ondown>
			<ondown condition="IsEmpty(Container(10).ListItemNoWrap(1).Label)">9001</ondown>
			<onleft>13</onleft>
			<ondown>14</ondown>
			<onright>noop</onright>
		</control>
		<!-- Background -->
		<include>DialogBackgroundImage</include>
		<!-- Main group -->
		<control type="group">
			<include>DialogAnimation</include>
			<!-- Heading -->
			<control type="label" id="2">
				<left>50</left>
				<top>40</top>
				<width>1820</width>
				<height>70</height>
				<font>Light60</font>
				<textcolor>$VAR[FocusedColor]</textcolor>
				<label>21432</label>
			</control>
			<!-- Left grouplist -->
			<control type="grouplist" id="100">
				<left>50</left>
				<top>155</top>
				<width>885</width>
				<height>800</height>
				<onright>10</onright>
				<onleft>noop</onleft>
				<ondown>noop</ondown>
				<onup>noop</onup>
				<itemgap>25</itemgap>
				<!-- Set Playlist type -->
				<control type="spincontrolex" id="22">
					<width>100%</width>
					<height>75</height>
					<label>467</label>
				</control>
				<!-- Name Button -->
				<control type="edit" id="12">
					<width>100%</width>
					<hinttext>21381</hinttext>
				</control>
				<!-- Default spin control -->
				<control type="spincontrolex" id="16">
					<width>100%</width>
					<label>21424</label>
				</control>
				<!-- Default spin control -->
				<control type="spincontrolex" id="17">
					<width>100%</width>
					<label>21427</label>
				</control>
				<!-- Default spin control -->
				<control type="spincontrolex" id="18">
					<width>100%</width>
					<label>21429</label>
				</control>
				<!-- Default toggle -->
				<control type="togglebutton" id="19">
					<width>100%</width>
					<label>$LOCALIZE[31107]: $LOCALIZE[21431]</label>
					<altlabel>$LOCALIZE[31107]: $LOCALIZE[21430]</altlabel>
				</control>
				<!-- Default spin control -->
				<control type="spincontrolex" id="23">
					<width>100%</width>
					<label>21458</label>
				</control>
				<!-- Default radio button -->
				<control type="radiobutton" id="24">
					<width>100%</width>
					<label>21459</label>
				</control>
			</control>
			<!-- Rules list -->
			<control type="list" id="10">
				<right>50</right>
				<top>155</top>
				<width>885</width>
				<height>800</height>
				<onup>noop</onup>
				<ondown>9001</ondown>
				<onleft>100</onleft>
				<onright>13</onright>
				<itemlayout width="885" height="100">
					<control type="image">
						<width>885</width>
						<height>75</height>
						<texture colordiffuse="$VAR[ButtonColor]">views/white_20.png</texture>
					</control>
					<control type="label">
						<left>10</left>
						<width>865</width>
						<height>75</height>
						<textcolor>$VAR[UnFocusedColor]</textcolor>
						<label>$INFO[ListItem.Label]</label>
					</control>
				</itemlayout>
				<focusedlayout width="885" height="100">
					<control type="group">
						<height>75</height>
						<visible>!Control.HasFocus(10) + !Control.HasFocus(13) + !Control.HasFocus(14)</visible>
						<control type="image">
							<width>885</width>
							<height>75</height>
							<texture colordiffuse="$VAR[ButtonColor]">views/white_20.png</texture>
						</control>
						<control type="label">
							<left>10</left>
							<width>865</width>
							<height>75</height>
							<textcolor>$VAR[UnFocusedColor]</textcolor>
							<label>$INFO[ListItem.Label]</label>
						</control>
					</control>
					<control type="group">
						<height>75</height>
						<visible>Control.HasFocus(10) | Control.HasFocus(13) | Control.HasFocus(14)</visible>
						<control type="image">
							<width>885</width>
							<height>75</height>
							<texture colordiffuse="$VAR[ButtonColor]">views/white_100.png</texture>
						</control>
						<control type="label">
							<left>10</left>
							<width>865</width>
							<height>75</height>
							<textcolor>$VAR[FocusedColor]</textcolor>
							<label>$INFO[ListItem.Label]</label>
							<visible>Control.HasFocus(10)</visible>
						</control>
						<control type="label">
							<left>10</left>
							<width>865</width>
							<height>75</height>
							<textcolor>$VAR[SelectedColor]</textcolor>
							<label>$INFO[ListItem.Label]</label>
							<visible>Control.HasFocus(13) | Control.HasFocus(14)</visible>
						</control>
					</control>
					<control type="image">
						<left>735</left>
						<width>75</width>
						<height>75</height>
						<texture colordiffuse="$VAR[UnFocusedColor]">dialogs/add.png</texture>
						<visible>!Control.HasFocus(13)</visible>
					</control>
					<control type="image">
						<left>810</left>
						<width>75</width>
						<height>75</height>
						<texture colordiffuse="$VAR[UnFocusedColor]">dialogs/delete.png</texture>
						<visible>!Control.HasFocus(14)</visible>
					</control>
					<control type="image">
						<left>735</left>
						<width>75</width>
						<height>75</height>
						<texture colordiffuse="$VAR[FocusedColor]">dialogs/add.png</texture>
						<visible>Control.HasFocus(13)</visible>
					</control>
					<control type="image">
						<left>810</left>
						<width>75</width>
						<height>75</height>
						<texture colordiffuse="$VAR[FocusedColor]">dialogs/delete.png</texture>
						<visible>Control.HasFocus(14)</visible>
					</control>
					<control type="image">
						<left>810</left>
						<width>75</width>
						<height>75</height>
						<texture colordiffuse="$VAR[DisabledColor]">dialogs/delete.png</texture>
						<visible>!Control.IsEnabled(14)</visible>
					</control>
				</focusedlayout>
			</control>
			<!-- Button grouplist -->
			<control type="grouplist" id="9001">
				<right>50</right>
				<bottom>50</bottom>
				<width>1200</width>
				<height>75</height>
				<itemgap>0</itemgap>
				<onleft>noop</onleft>
				<onright>noop</onright>
				<onup>10</onup>
				<ondown>noop</ondown>
				<align>right</align>
				<orientation>horizontal</orientation>
				<!-- Cancel Button -->
				<control type="button" id="21">
					<width>75</width>
					<label>X</label>
					<align>center</align>
				</control>
				<!-- OK Button -->
				<control type="button" id="20">
					<width>150</width>
					<label>186</label>
					<align>center</align>
				</control>
			</control>
		</control>
	</controls>
</window>