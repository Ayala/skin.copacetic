<?xml version="1.0" encoding="UTF-8"?>
<window>
	<!-- contentsettings -->
	<defaultcontrol always="true">20</defaultcontrol>
	<controls>
		<!-- Background -->
		<include>DialogBackgroundImage</include>
		<!-- Main group -->
		<control type="group" id="1000">
			<include name="DialogHeading">
				<param name="DialogWidth" value="1820" />
				<param name="DialogHeaderId" value="-" />
				<param name="DialogHeaderLabel" value="$LOCALIZE[20333]" />
			</include>
			<!-- Grouplist -->
			<control type="grouplist">
				<width>1200</width>
				<include>DialogGrouplist</include>
				<!-- Content Picker -->
				<control type="button" id="20" />
				<!-- Content provider -->
				<control type="button" id="21" />
				<!-- Control grouplist -->
				<control type="grouplist" id="5">
					<height>400</height>
					<itemgap>25</itemgap>
					<orientation>vertical</orientation>
				</control>
				<!-- Default button -->
				<control type="button" id="7" />
				<!-- Default radiobutton -->
				<control type="radiobutton" id="8" />
			</control>
		</control>
		<include name="DialogButtons">
			<param name="Button4Visible" value="false" />
			<!-- Settings Button -->
			<param name="Button3ID" value="22" />
			<param name="Button3Label" value="$LOCALIZE[5]" />
			<param name="Button3Visible" value="true" />
			<!-- Cancel Button -->
			<param name="Button2ID" value="29" />
			<param name="Button2Label" value="X" />
			<param name="Button2Visible" value="true" />
			<!-- OK Button -->
			<param name="Button1ID" value="28" />
			<param name="Button1Label" value="$LOCALIZE[186]" />
			<param name="Button1Visible" value="true" />
		</include>
	</controls>
</window>
